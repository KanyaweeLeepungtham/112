#include <stdio.h>
int maxi(int a[], int n) {
    int max = a[0];
    for (int i = 1; i < n; i++) {
        if (a[i] > max) {
            max = a[i];
        }
    }
    return max;
}

int main() {
    int n;
    // printf(" ");
    scanf("%d", &n);

    int a[n];
    // printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    int result = maxi(a, n);
    printf("Maximum value: %d\n", result);

    return 0;
}
