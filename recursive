#include <stdio.h>
int maxi(int a[], int n) {
    if (n == 1) {
        return a[0];
    }
    if (a[n - 1] > maxi(a, n - 1)){
        return a[n-1];
    }
    else{
        return maxi(a, n - 1);
    }
}

int main() {
    int n;
    scanf("%d", &n);

    int a[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    printf("Maximum value: %d\n", maxi(a, n));

    return 0;
}
#include <stdio.h>
int parlin(char word[], int start, int end) {
    if (start >= end) {
        return 1; 
    }
    if (word[start] != word[end]) {
        return 0; 
    }
    return parlin(word, start + 1, end - 1);
}

int main() {
    char word[30];
    scanf("%s", word);
    int len =0;
    while (word[len] != '\0') {
        len++;
    }
    if (parlin(word, 0, len - 1)) {
        printf("Palindrome\n");
    } else {
        printf("Not palindrome\n");
    }
    return 0;
}
