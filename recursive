#include <stdio.h>

// ฟังก์ชัน recursive สำหรับหาค่าสูงสุดในอาร์เรย์
int maxi_recursive(int a[], int n) {
    if (n == 1) {
        return a[0]; // base case
    }

    int max_rest = maxi_recursive(a, n - 1); // หาค่าสูงสุดจาก n-1 ตัวแรก
    return (a[n - 1] > max_rest) ? a[n - 1] : max_rest; // เปรียบเทียบกับตัวสุดท้าย
}

int main() {
    int n;
    scanf("%d", &n);

    int a[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    int result = maxi_recursive(a, n);
    printf("Maximum value: %d\n", result);

    return 0;
}

#include <stdio.h>
#include <string.h>

// ฟังก์ชัน recursive ตรวจสอบว่าเป็น palindrome หรือไม่
int parlin_recursive(char word[], int start, int end) {
    if (start >= end) {
        return 1; // base case: ตัวอักษรชนกันแล้ว (หรือเลยกัน) แสดงว่าเป็น palindrome
    }

    if (word[start] != word[end]) {
        return 0; // ถ้าไม่เท่ากัน ไม่เป็น palindrome
    }

    // เรียกซ้ำโดยเลื่อนเข้าไปหาตรงกลาง
    return parlin_recursive(word, start + 1, end - 1);
}

int main() {
    char word[30];
    scanf("%s", word);

    int len = strlen(word);
    if (parlin_recursive(word, 0, len - 1)) {
        printf("Palindrome\n");
    } else {
        printf("Not palindrome\n");
    }

    return 0;
}