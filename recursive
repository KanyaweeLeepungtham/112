#include <stdio.h>
int maxi(int a[], int n) {
    int max = a[0];
    for (int i = 1; i < n; i++) {
        if (a[i] > max) {
            max = a[i];
        }
    }
    return max;
}

int main() {
    int n;
    // printf(" ");
    scanf("%d", &n);

    int a[n];
    // printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }
    int result = maxi(a, n);
    printf("Maximum value: %d\n", result);

    return 0;
}
//
//
//
#include <stdio.h>
int parlin(char word[]) {
    int len = 0;
    while (word[len] != '\0') {
        len++;
    }
    for (int i = 0; i < len / 2; i++) {
        if (word[i] != word[len - 1 - i]) {
            return 0; // ไม่เป็น palindrome
        }
    }

    return 1; // เป็น palindrome
}

int main() {
    char word[30];
    scanf("%s", word);
    // เรียกใช้ฟังก์ชันและตรวจผลลัพธ์ที่คืนมา
    if (parlin(word)) {
        printf("Palindrome\n");
    } else {
        printf("Not palindrome\n");
    }
}
